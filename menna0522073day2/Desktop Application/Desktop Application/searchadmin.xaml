<Page x:Class="Desktop_Application.searchadmin"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Desktop_Application"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="searchadmin">

    <Grid Background="LightBlue">
        <Button Content="search" HorizontalAlignment="Left" Margin="602,16,0,0" VerticalAlignment="Top" Width="94" FontSize="20" Background="DarkBlue" Click="Button_Click"/>
        <TextBox x:Name="deletcity" HorizontalAlignment="Left" Margin="169,13,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="277" Height="31" FontSize="20"/>
        <Label Content="City" HorizontalAlignment="Left" Margin="10,10,10,10" VerticalAlignment="Top" Background="DarkBlue" Width="57" FontSize="20"/>
        <DataGrid x:Name="dg" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="0,126,0,0"/>

    </Grid>
</Page>
